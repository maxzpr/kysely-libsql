{
  "name": "@maxzpr/kysely-libsql-client",
  "version": "0.0.1",
  "keywords": [
    "libsql",
    "turso",
    "database",
    "sqlite",
    "kysely"
  ],
  "description": "Kysely dialect for libSQL",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxzpr/kysely-libsql-client.git"
  },
  "homepage": "https://github.com/maxzpr/kysely-libsql-client",
  "authors": [
    "Maxzpr"
  ],
  "license": "MIT",
  "type": "module",
  "main": "lib-cjs/index.js",
  "types": "lib-esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib-esm/index.d.ts",
      "import": "./lib-esm/index.js",
      "require": "./lib-cjs/index.js"
    }
  },
  "files": [
    "lib-cjs/**",
    "lib-esm/**"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rm -rf ./lib-cjs ./lib-esm",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.build-cjs.json",
    "build:esm": "tsc -p tsconfig.build-esm.json",
    "postbuild": "cp package-cjs.json ./lib-cjs/package.json",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@libsql/client": "^0.6.0"
  },
  "peerDependencies": {
    "kysely": "*"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "^29.4.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  }
}
